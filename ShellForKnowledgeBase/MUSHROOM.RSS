GOAL:	EDIBMSH

INITIAL:
	e.lstr=80
	e.tryp = "e"
	CLEAR
	LOAD FORMV2
	LOAD FORMOT1
	LOAD FORMOT2
	LOAD VXOD
	PUTFORM VXOD
	WAIT
	RELEASE VXOD
	CLEAR
	EDIBMSH=UNKNOWN
	COLORHB=UNKNOWN
	WAVCOLOR=UNKNOWN
	MOHD=UNKNOWN
	BORINED=UNKNOWN
	COLORLF=UNKNOWN
	AMAN=UNKNOWN
	FLUFF=UNKNOWN
	BOR=UNKNOWN
	COLORL=UNKNOWN
	CLASSLAM=UNKNOWN
	SKIRT=UNKNOWN
	WARTS=UNKNOWN
	COLORHF=UNKNOWN
	COLORSLC=UNKNOWN
	BORLEG=UNKNOWN
	KINDMSH=UNKNOWN
	SKIN=UNKNOWN
	STRUCTSH=UNKNOWN
	HATSH=UNKNOWN
	COLORH=UNKNOWN
	DIM VARR(6)
	USE MSHOTV
	USE MSHVOPR
	
DO:
	CLEAR
	IF NOT(KNOWN("EDIBMSH")) THEN
		LOAD FORMOT2
		PUTFORM FORMOT2
		WAIT
		RELEASE FORMOT2
	ELSE
		LOAD FORMOT1
		PUTFORM FORMOT1
		TALLY FORMOT1
		RELEASE FORMOT1
	ENDIF
	
RULE:	RKIND1
	IF:	STRUCTSH=1 AND HATSH=1
	THEN:	KINDMSH=""
	NEEDS:	STRUCTSH
		HATSH
	CHANGES: KINDMSH
	REASON:            

RULE:	RKIND2
	IF:	STRUCTSH=2
	THEN:	KINDMSH=""
	NEEDS:	STRUCTSH
	CHANGES: KINDMSH
	REASON:         
	
RULE:	RBOR1
	IF:	SKIN=1
	THEN:	BOR=""
	NEEDS:	SKIN
	CHANGES: BOR
	REASON:         
	
RULE:	RBOR2
	IF:	KINDMSH="" AND BORLEG=1 AND (COLORH=1 OR COLORH=2 OR COLORH=3 OR COLORH=4 OR COLORH=5)
	THEN:	BOR=""
	NEEDS:	KINDMSH
		BORLEG
		COLORH
	CHANGES: BOR
	REASON:         , , ,     
	
RULE:	RBOR3
	IF:	KINDMSH="" AND COLORH=6 AND COLORHF=1
	THEN:	BOR=""
	NEEDS:	KINDMSH
		COLORH
		COLORHF
	CHANGES: BOR
	REASON:            
	
RULE:	REDMSH1
	IF:	BOR=""
	THEN:	EDIBMSH=""
	NEEDS:	BOR
	CHANGES: EDIBMSH
	REASON:   
	
RULE:	REDMSH2
	IF:	BORINED="" AND COLORLF=1
	THEN:	EDIBMSH=""
	NEEDS:	BORINED
		COLORLF
	CHANGES: EDIBMSH
	REASON:           
	
RULE:	REDMSH3
	IF:	BORINED="" AND COLORLF<>1
	THEN:	EDIBMSH=""
	NEEDS:	BORINED
		COLORLF
	CHANGES: EDIBMSH
	REASON:          

RULE:	REDMSH4
	IF:	BORINED=""
	THEN:	EDIBMSH=""
	NEEDS:	BORINED
	CHANGES: EDIBMSH
	REASON:   
	
RULE:	REDMSH5
	IF:	BOR="" AND MOHD=1
	THEN:	EDIBMSH=""
	NEEDS:	BOR
		MOHD
	CHANGES: EDIBMSH
	REASON:         
	
RULE:	REDMSH6
	IF:	BOR="" AND MOHD=2
	THEN:	EDIBMSH=""
	NEEDS:	BOR
		MOHD
	CHANGES: EDIBMSH
	REASON:    
	
RULE:	REDMSH7
	IF:	KINDMSH="" AND COLORH=3 AND COLORHB=1 AND COLORHF=3 AND COLORSLC=2
	THEN:	EDIBMSH=""
	NEEDS:	KINDMSH
		COLORH
		COLORHB
		COLORHF
		COLORSLC
	CHANGES: EDIBMSH
	REASON:                 
	
RULE:	REDMSH8
	IF:	KINDMSH="" AND COLORSLC=2 AND (COLORL=1 OR (COLORL=2 AND COLORLF=1)) AND COLORH=3 AND COLORHF=1
	THEN:	EDIBMSH="-"
	NEEDS:	KINDMSH
		COLORSLC
		COLORL
		COLORLF
		COLORH
		COLORHF
	CHANGES: EDIBMSH
	REASON:                          - 
	
RULE:	REDMSH9
	IF:	KINDMSH="" AND COLORSLC=2 AND (COLORH=3 OR COLORH=4) AND COLORHB=2
	THEN:	EDIBMSH=""
	NEEDS:	KINDMSH
		COLORSLC
		COLORH
		COLORHB
	CHANGES: EDIBMSH
	REASON:                      

RULE:	REDMSH10
	IF:	CLASSLAM=" " AND WAVCOLOR=1 AND (COLORH=7 OR COLORH=4) AND FLUFF=1
	THEN:	EDIBMSH=""
	NEEDS:	CLASSLAM
		WAVCOLOR
		COLORH
		FLUFF
	CHANGES: EDIBMSH
	REASON:              
	
RULE:	REDMSH11
	IF:	CLASSLAM=" " AND WAVCOLOR=2 AND COLORH=1 AND FLUFF=1
	THEN:	EDIBMSH=""
	NEEDS:	CLASSLAM
		WAVCOLOR
		COLORH
		FLUFF
	CHANGES: EDIBMSH
	REASON:             

RULE:	REDMSH12
	IF:	CLASSLAM=" " AND WAVCOLOR=1 AND FLUFF=2
	THEN:	EDIBMSH=""
	NEEDS:	CLASSLAM
		WAVCOLOR
		FLUFF
	CHANGES: EDIBMSH
	REASON:              
	
RULE:	REDMSH13
	IF:	AMAN=""
	THEN:	EDIBMSH=""
	NEEDS:	AMAN
	CHANGES: EDIBMSH
	REASON:  
	
RULE:	REDMSH14
	IF:	CLASSLAM=" " AND WAVCOLOR=2 AND SKIRT=2 AND WARTS=2 AND COLORH=7
	THEN:	EDIBMSH="-"
	NEEDS:	CLASSLAM
		WAVCOLOR
		SKIRT
		WARTS
		COLORH
	CHANGES: EDIBMSH
	REASON:         ,     - 

RULE:	REDMSH15
	IF:	CLASSLAM=" " AND WAVCOLOR=2 AND SKIRT=2 AND WARTS=2 AND COLORH=8
	THEN:	EDIBMSH=""
	NEEDS:	CLASSLAM
		WAVCOLOR
		SKIRT
		WARTS
		COLORH
	CHANGES: EDIBMSH
	REASON:         ,      

RULE:	RBORED1
	IF:	BOR="" AND (COLORH=1 OR COLORH=2) AND (COLORL=2 OR COLORL=1)
	THEN:	BORINED=""
	NEEDS:	BOR
		COLORH
		COLORL
	CHANGES: BORINED
	REASON:              
	
RULE:	RBORED2
	IF:	BOR="" AND (COLORHF=3 OR COLORH=1) AND COLORSLC=1
	THEN:	BORINED=""
	NEEDS:	BOR
		COLORHF
		COLORSLC
	CHANGES: BORINED
	REASON:               

RULE:	RBORED3
	IF:	BOR="" AND COLORL=1 AND COLORH=3 AND COLORSLC=2
	THEN:	BORINED=""
	NEEDS:	BOR
		COLORL
		COLORH
		COLORSLC
	CHANGES: BORINED
	REASON:             
	
RULE:	RBORED4
	IF:	BOR="" AND COLORSLC=3
	THEN:	BORINED=""
	NEEDS:	BOR
		COLORSLC
	CHANGES: BORINED
	REASON:        
	
RULE:	RBORED5
	IF:	BOR="" AND COLORL<>1 AND COLORH<>3 AND COLORSLC=2
	THEN:	BORINED=""
	NEEDS:	BOR
		COLORL
		COLORH
		COLORSLC
	CHANGES: BORINED
	REASON:                

RULE:	RBORED6
	IF:	KINDMSH="" AND COLORSLC=3
	THEN:	BORINED=""
	NEEDS:	KINDMSH
		COLORSLC
	CHANGES: BORINED
	REASON:          
	
RULE:	RCLASSL1
	IF:	KINDMSH="" AND HATSH=3
	THEN:	CLASSLAM=" "
	NEEDS:	KINDMSH
		HATSH
	CHANGES: CLASSLAM
	REASON:             
	
RULE:	RCLASSL2
	IF:	KINDMSH="" AND HATSH=1
	THEN:	CLASSLAM=" "
	NEEDS:	KINDMSH
		HATSH
	CHANGES: CLASSLAM
	REASON:           

RULE:	RAMAN1
	IF:	CLASSLAM=" " AND SKIRT=1 AND WARTS=1
	THEN:	AMAN=""
	NEEDS:	CLASSLAM
		SKIRT
		WARTS
	CHANGES: AMAN
	REASON:              
	
RULE:	RAMAN2
	IF:	CLASSLAM=" " AND SKIRT=1 AND WARTS=2 AND (COLORH=1 OR COLORH=6)
	THEN:	AMAN=""
	NEEDS:	CLASSLAM
		SKIRT
		WARTS
		COLORH
	CHANGES: AMAN
	REASON:                  

RULE:	RAMAN3
	IF:	CLASSLAM=" " AND SKIRT=2 AND WARTS=1 AND (COLORH=4 OR COLORH=6 OR COLORH=8)
	THEN:	AMAN=""
	NEEDS:	CLASSLAM
		SKIRT
		WARTS
		COLORH
	CHANGES: AMAN
	REASON:    ,            

VAR:	BORINED
	LABEL:	 

VAR:	BOR
	LABEL:	 
	
VAR:	AMAN
	LABEL:	    
	
VAR:	CLASSLAM
	LABEL:	  
	
VAR:	KINDMSH
	LABEL:	    

VAR:	STRUCTSH
	FIND:	PERFORM PROC1 USING "FORMV2","1","STRUCTSH","2"
	LABEL:	   

VAR:	HATSH
	FIND:	PERFORM PROC1 USING "FORMV2","2","HATSH","3"
	LABEL:	 
	
VAR:	BORLEG
	FIND:	PERFORM PROC1 USING "FORMV2","3","BORLEG","3"
	LABEL:	 

VAR:	SKIN
	FIND:	PERFORM PROC1 USING "FORMV2","4","SKIN","2"
	LABEL:	   
	
VAR:	COLORH
	FIND:	PERFORM PROC1 USING "FORMV2","5","COLORH","8"
	LABEL:	 

VAR:	COLORSLC
	FIND:	PERFORM PROC1 USING "FORMV2","6","COLORSLC","4"
	LABEL:	   
	
VAR:	COLORHF
	FIND:	PERFORM PROC1 USING "FORMV2","7","COLORF","4"
	LABEL:	 
	
VAR:	COLORL
	FIND:	PERFORM PROC1 USING "FORMV2","8","COLORL","3"
	LABEL:	 
	
VAR:	COLORLF
	FIND:	PERFORM PROC1 USING "FORMV2","9","COLORLF","2"
	LABEL:	  
	
VAR:	MOHD
	FIND:	PERFORM PROC1 USING "FORMV2","10","MOHD","2"
	LABEL:	 
	
VAR:	COLORHB
	FIND:	PERFORM PROC1 USING "FORMV2","11","COLORHB","3"
	LABEL:	     
	
VAR:	SKIRT
	FIND:	PERFORM PROC1 USING "FORMV2","12","SKIRT","2"
	LABEL:	   
	
VAR:	WARTS
	FIND:	PERFORM PROC1 USING "FORMV2","13","WARTS","2"
	LABEL:	     
	
VAR:	FLUFF
	FIND:	PERFORM PROC1 USING "FORMV2","14","FLUFF","2"
	LABEL:	      
	
VAR:	WAVCOLOR
	FIND:	PERFORM PROC1 USING "FORMV2","15","WAVCOLOR","2"
	LABEL:	    

END:
